{
  "version": 2,
  "builds": [
    {
      "src": "api/pln-inquiry.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/pln-inquiry",
      "dest": "api/pln-inquiry.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
